    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/(components|screens)/*.{js,jsx,ts,tsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/jest.setup.js"
    ]
  },
