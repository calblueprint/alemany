{
  "extends": "./node_modules/expo/tsconfig.base",
  "compilerOptions": {
    "module": "esnext",
    "jsx": "react-native",
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "src/*": ["src/*"],
      "assets/*": ["assets/*"],
      "components/*": ["src/components/*"],
      "constants/*": ["src/constants/*"],
      "database/*": ["src/database/*"],
      "hooks/*": ["src/hooks/*"],
      "navigation/*": ["src/navigation/*"],
      "screens/*": ["src/screens/*"],
      "@types": ["types.tsx"]
    }
  }
}
